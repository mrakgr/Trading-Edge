<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Types.fs" />
    <Compile Include="Config.fs" />
    <Compile Include="S3Download.fs" />
    <Compile Include="SplitDownload.fs" />
    <Compile Include="IntradayDownload.fs" />
    <Compile Include="TradesDownload.fs" />
    <Compile Include="QuotesDownload.fs" />
    <Compile Include="Conditions.fs" />
    <Compile Include="TradeMetrics.fs" />
    <Compile Include="Database.fs" />
    <Compile Include="Plotting.fs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="sql/schema/tables/*.sql" />
